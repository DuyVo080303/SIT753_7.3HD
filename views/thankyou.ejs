<%- include('header'); -%>

    <header>
        <nav class="navbar navbar-expand-lg bg-dark text-white sticky-top">
            <div class="container">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation" style="background-color: white;">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <a class="navbar-brand text-white" aria-current="page" href="/home">
                    <img src="Logo.png" height="30" width="30" alt="Figure Zone Logo">
                </a>
                <a>
                    <span class="search" data-bs-toggle="collapse" href="#collapseEx" role="button"
                        aria-expanded="false" aria-controls="collapseEx">Search
                        <img src="icons8-magnifying-glass-50.png" alt="magnifying glass symbol" width="20" height="20">
                    </span>
                    <span>Cart
                        <img src="New Arrivals/shopping bag.png" alt="shopping bag symbol" width="20" height="20">
                    </span>
                    <span>
                        <a id="user" class="nav-link text-white" href="/login" role="button">
                            Login
                            <img src="New Arrivals/user.png" alt="user symbol" width="20" height="20">
                        </a>
                    </span>

                </a>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link text-white" href="/home">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" href="/about">About Us</a>
                        </li>
                        <li class="nav-item dropdown d-flex">
                            <a class="nav-link nav-link-product text-white" href="/product" role="button">
                                Product
                                <a class="nav-link nav-link-drop dropdown-toggle text-white" role="button"
                                    data-bs-toggle="dropdown" aria-expanded="false">
                                </a>
                                <ul class="dropdown-menu">
                                    <li><a class="dropdown-item" href="/figure">Figure</a></li>
                                    <li><a class="dropdown-item" href="/tcg">Trading Card Game</a></li>
                                </ul>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" href="/social">News</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" href="/help">Help & Support</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <div class="collapse" id="collapseEx">
            <div class="fd">
                <div class="find">
                    <span class="material-symbols-outlined">
                        search
                    </span>
                    <input class="search-input" type="search" placeholder="Search">
                </div>
            </div>
        </div>
    </header>

    <main>
        <div class="container">
            <h3 class="mt-3"> Form submmited successfully</h3>
            <P>
                Thank you for your submittion
            </P>

            <h4>
                <%= title %>
            </h4>

            <table class="table table-bordered">
                <tr>
                    <th>Firstname</th>
                    <td>
                        <%= fname %>
                    </td>
                </tr>

                <tr>
                    <th>Surname</th>
                    <td>
                        <%= sname %>
                    </td>
                </tr>

                <tr>
                    <th>Email</th>
                    <td>
                        <%= email %>
                    </td>
                </tr>

                <tr>
                    <th>Mobile</th>
                    <td>
                        <%= mobile %>
                    </td>
                </tr>

                <tr>
                    <th>Address</th>
                    <td>
                        <%= ad %>
                    </td>
                </tr>

                <tr>
                    <th>City</th>
                    <td>
                        <%= city %>
                    </td>
                </tr>

                <tr>
                    <th>State</th>
                    <td>
                        <%= state %>
                    </td>
                </tr>

                <tr>
                    <th>Zip</th>
                    <td>
                        <%= zip %>
                    </td>
                </tr>


                <tr>
                    <th>Comments</th>
                    <td>
                        <%= cmt %>
                    </td>
                </tr>
            </table>

        </div>
    </main>

    <%- include('footer'); -%>